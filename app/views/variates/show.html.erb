<%- model_class = Variate -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:experiment_id) %></dt>
      <dd><pre class="prettyprint"><%= @variate.experiment_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:variate_no) %></dt>
      <dd><pre class="prettyprint"><%= @variate.variate_no %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:js_code) %></dt>
      <dd><pre class="prettyprint"><%= @variate.js_code %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              @experiment, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_variate_path(@variate), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              variate_path(@variate),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

<h2>Variate Page</h2>
<iframe src="<%= @experiment.page_url %>" style="width:100%; height:1000px;"></iframe>